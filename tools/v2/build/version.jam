rule boost-build ( )
{
    return "V2 (Milestone 10)" ;
}
rule jam ( )
{
    local v = [ modules.peek : JAM_VERSION  ] ;
    return $(v:J=.) ;
}


rule print ( )
{
    ECHO "Boost.Build" [ boost-build ] ;
    ECHO "Boost.Jam" [ jam ] ;
}
